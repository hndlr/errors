{
  "name": "@hndlr/errors",
  "version": "0.1.0-beta.1",
  "description": "JS-less NPM Package that builds on publish",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "module": "./src/index.js",
  "repository": {
    "url": "git+https://github.com/hndlr/errors.git"
  },
  "scripts": {
    "prebuild": "error build",
    "build": "tsc && tsc -d",
    "prebuild:test": "npm run prebuild -- --test",
    "build:test": "tsc",
    "pretest": "npm run build:test",
    "test": "mocha --require @babel/register --require @babel/polyfill --exit",
    "posttest": "rm -rf ./tsconfig.json && rm -rf ./src && rm -rf ./test",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "prepublishOnly": "npm run build",
    "postpublish": "rm -rf ./tsconfig.json && rm -rf ./src"
  },
  "keywords": [
    "http",
    "errors"
  ],
  "mocha": {
    "ui": "bdd",
    "reporter": "dot"
  },
  "author": "Harry Wright",
  "license": "ISC",
  "dependencies": {
    "http-status-codes": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.22.20",
    "@babel/register": "^7.22.15",
    "@hndlr/template": "^0.1.0-beta.1",
    "@istanbuljs/nyc-config-babel": "^3.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^6.1.1",
    "chai": "^4.3.9",
    "cross-env": "^7.0.3",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "standard": "^17.1.0",
    "typescript": "^5.2.2"
  }
}
